<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="GraphyPCL.UserDataPage">
  <TableView x:Name="_tableView">
    <TableView.Root>
      <TableRoot x:Name="_tableRoot">
        <TableSection Title="Search Data">
          <EntryCell Text="{Binding UserData.TagSearchCount}" Label="Tag Search:" />
          <EntryCell Text="{Binding UserData.TagUsedInSearchCount}" Label="Tag Used in Search:" />
          <EntryCell Text="{Binding UserData.RelationshipSearchCount}" Label="Relationship Search:" />
          <EntryCell Text="{Binding UserData.RelationshipUsedInSearchCount}" Label="Relationship Used in Search:" />
          <EntryCell Text="{Binding UserData.AllSearchCount}" Label="All Search:" />
          <EntryCell Text="{Binding UserData.RelationshipNavigationCount}" Label="Relationship Navigation:" />
        </TableSection>
      </TableRoot>
    </TableView.Root>
  </TableView>
</ContentPage>