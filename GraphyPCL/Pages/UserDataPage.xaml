          <TextCell Detail="{Binding TagAverageActive}" Text="TagAverageActive:" />
<!--          <TextCell Detail="{Binding TagStandardDeviation}" Text="TagStandardDeviation:" />-->
          <TextCell Detail="{Binding TagStandardDeviationActive}" Text="TagStandardDeviationActive:" />
